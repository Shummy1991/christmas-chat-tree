{
  "name": "christmas-chat-tree",
  "version": "1.0.0",
  "description": "Build your own chat in React with Server-Sent Events & OneSignal",
  "main": "src/client.js",
  "engines": {
    "node": ">=10.12.0",
    "npm": ">=6.4.1"
