  "scripts": {
    "lint": "eslint ./src",
    "dev": "cross-env NODE_ENV=development babel-node ./app.js",
    "dev:client": "cross-env NODE_ENV=development RENDERER=true babel-node ./app.js",
    "dev:server": "nodemon --exec cross-env NODE_ENV=development RENDERER=true babel-node src/server/app.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "rimraf public && webpack --config ./webpack.config.prod.client.js",
    "build:server": "webpack --config ./webpack.config.prod.server.js",
    "start": "cross-env NODE_ENV=production node ./server.bundle.js",
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "bs": "npm run build && npm run start"
  },
